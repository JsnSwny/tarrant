{
	"introduction": {
		"EXEC": "this.utter('introduction')",
		"agreement": "this.say('Awesome! Then let us begin.'); this.nextQuestion()",
		"reject": "this.say('Too bad. Maybe next time then.')",
		"DEFAULT": ""
	},
	"invite": {
		"EXEC": "this.say('Would you like to play Who Wants To Be A Millionaire?')",
		"DEFAULT": "this.say('Sorry. I do not quite understand your answer. Was that a yes?')"
	},
	"goodbye": {
		"EXEC": "this.say('Catch you later.')"
	},
	"question": {
		"EXEC": "this.stateQuestion(this.stateArgs)",
		"offer-answer": "this.handleOfferAnswer(this.lastIntent.args)",
		"reject-option": "this.handleRejectOption(this.lastIntent.args)",
		"confirm-final-answer": "this.changeState('accept-answer')",
		"agreement": "(this.answerOffered === '') ? '' : this.changeState('seek-confirmation')",
		"final-answer": "(this.answerOffered === '') ? this.utter('seek-answer-clarification') : this.changeState('accept-answer')",
		"DEFAULT": "",
		"SILENCE": [10, "this.offerGuidance()"]
	},
	"seek-confirmation": {
		"EXEC": "this.utter('seek-confirmation', [this.answerOffered])",
		"agreement": "this.changeState('accept-answer')",
		"final-answer": "this.changeState('accept-answer')",
		"reject": "this.changeState('question', [false])",
		"reject-option": "(this.lastIntent.args[0] === this.answerOffered) ? this.changeState('question', [false]) : this.changeState('accept-answer')",
		"offer-answer": "this.handleOfferAnswer(this.lastIntent.args)",
		"offer-to-answer": "this.utter('encourage'); this.changeState('question', [false])",
		"confirm-final-answer": "this.changeState('accept-answer')",
		"DEFAULT": [8, "this.utter('return-to-question'); this.changeState('question', [false])"],
		"SILENCE": [6, "this.utter('return-to-question'); this.changeState('question', [false])"]
	},
	"accept-answer": {
		"EXEC": "this.utter('repeat-answer', [this.answerOffered]); this.acceptAnswer()"
	},
	"end-of-game": {
		"EXEC": "this.utter('end-of-game', [this.correctlyAnswered, this.totalQuestions, this.winnings]); this.handleEndOfGame()",
		"DEFAULT": ""
	}
}
